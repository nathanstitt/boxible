{
  "name": "boxible",
  "version": "1.5.1",
  "description": "Typescript React component to set flexbox properties on a element",
  "repository": "https://github.com/nathanstitt/boxible",
  "author": "Nathan Stitt",
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "exports": {
    "import": "./dist/index.modern.js",
    "require": "./dist/index.js"
  },
  "unpkg": "dist/index.umd.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --no-compress",
    "build:tsc": "tsc",
    "build:tsc:watch": "tsc --watch",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "test:unit": "jest",
    "test": "run-s test:unit lint test:build",
    "test:build": "run-s build",
    "postpublish": "./script/post-publish",
    "lint": "eslint src/*ts"
  },
  "peerDependencies": {
    "@emotion/styled": "*",
    "react": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/jest": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.3.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "babel-jest": "^28.1.2",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.0",
    "eslint": "8.19.0",
    "eslint-config-argosity": "3.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.2",
    "microbundle": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "~18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^28.0.5",
    "typedoc": "^0.23.5",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "jest-environment-jsdom": "^28.1.2"
